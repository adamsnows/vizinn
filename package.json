{
  "name": "vizinn-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Vizinn monorepo containing frontend and backend applications",
  "scripts": {
    "dev:app": "pnpm --filter frontend-app run start",
    "dev:backend": "cd backend && poetry run uvicorn backend.app:app --reload",
    "dev": "pnpm -r --workspace-concurrency Infinity run dev",
    "build:app": "pnpm --filter frontend-app run build",
    "build:packages": "pnpm --filter \"./packages/**\" run build",
    "build": "pnpm build:packages && pnpm build:app",
    "lint:app": "pnpm --filter frontend-app run lint",
    "test:app": "pnpm --filter frontend-app run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
